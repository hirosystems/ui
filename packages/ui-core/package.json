{
  "name": "@stacks/ui-core",
  "version": "7.3.0",
  "author": "Thomas Osmonson",
  "dependencies": {
    "@styled-system/should-forward-prop": "^5.1.5",
    "camelcase": "^6.0.0",
    "csstype": "^3.0.8",
    "micro-memoize": "^4.0.9"
  },
  "devDependencies": {
    "@babel/core": "7.13.15",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/plugin-transform-react-jsx": "7.13.12",
    "@babel/plugin-transform-typescript": "7.13.0",
    "@babel/preset-env": "7.13.15",
    "@emotion/babel-plugin": "^11.3.0",
    "@emotion/core": "^11.0.0",
    "@emotion/react": "^11.1.1",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-node-resolve": "^11.2.1",
    "@rollup/plugin-typescript": "^8.2.1",
    "@stacks/prettier-config": "^0.0.7",
    "@types/lodash-es": "^4.17.3",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "husky": "^4.2.5",
    "json": "^10.0.0",
    "prettier": "^2.2.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "rollup": "^2.45.2",
    "rollup-plugin-size-snapshot": "^0.12.0",
    "shx": "^0.3.3",
    "tslib": "^2.2.0",
    "typescript": "^4.2.4"
  },
  "engines": {
    "node": ">=12"
  },
  "files": [
    "dist",
    "src"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "peerDependencies": {
    "@emotion/core": "^11.0.0",
    "@emotion/react": "^11.1.1",
    "react": "^17.0.1"
  },
  "prettier": "@blockstack/prettier-config",
  "private": false,
  "scripts": {
    "build": "rollup --config ./rollup.config.js",
    "prebuild": "shx rm -rf dist",
    "postbuild": "yarn copy",
    "types": "tsc --project tsconfig.json",
    "lint": "tsdx lint",
    "prepublishOnly": "yarn build",
    "start": "tsdx watch --verbose --noClean --onSuccess yalc publish --push",
    "test": "tsdx test --passWithNoTests",
    "copy": "shx cp -r dist/src/* dist/esm && shx mv dist/src/* dist && shx rm -rf dist/src && shx rm -rf tsconfig.tsbuildinfo && shx cp package.json dist && json -I -f dist/package.json -e \"this.private=false; this.devDependencies=undefined; this.optionalDependencies=undefined; this.scripts=undefined; this.husky=undefined; this.prettier=undefined; this.jest=undefined; this['lint-staged']=undefined;\""
  },
  "sideEffects": false,
  "typings": "dist/types/index.d.ts",
  "umd:main": "dist/ui-core.umd.production.js",
  "unpkg": "dist/ui-core.cjs.production.min.js",
  "publishConfig": {
    "access": "public"
  }
}
