    "use-onclickoutside": "npm:use-onclickoutside-peer-deps@0.3.1"
  },
  "devDependencies": {
    "@blockstack/prettier-config": "^0.0.6",
    "@emotion/core": "^11.0.0",
    "@emotion/react": "^11.1.1",
    "@types/lodash-es": "^4.17.3",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-transition-group": "^4.2.4",
